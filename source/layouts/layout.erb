<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="A layout example that shows off a responsive product landing page.">

  <title>Erica Lauer Vose Portfolio</title>

  <link rel="stylesheet" href="http://yui.yahooapis.com/pure/0.3.0/pure-min.css">

  <link rel="stylesheet" href="stylesheets/marketing.css">

</head>
<body>
  <div class="content">
    <div class="header">
      <div class="pure-menu pure-menu-open pure-menu-fixed pure-menu-horizontal">
        <a class="pure-menu-heading" href="">Erica lauer Vose</a>

        <ul>
          <li class="pure-menu-selected"><a href="#">Home</a></li>
          <li><a href="#">Projects</a></li>
          <li><a href="images/erica_lauer_vose_resume.pdf" target="_blank">Resume</a></li>
          <!-- <li><a href="#">Blog</a></li> -->
        </ul>
      </div>
    </div>

    <%= yield %>

  <div class="footer">
    View the source of this layout to learn more. Made with love by the YUI Team.
  </div>
  </div>

  <script src="http://yui.yahooapis.com/3.12.0/build/yui/yui.js"></script>
  <script>
  YUI().use('node-base', 'node-event-delegate', function (Y) {
  // This just makes sure that the href="#" attached to the <a> elements
  // don't scroll you back up the page.
  Y.one('body').delegate('click', function (e) {
    e.preventDefault();
  }, 'a[href="#"]');
  });
  </script>

  </body>
  </html>
